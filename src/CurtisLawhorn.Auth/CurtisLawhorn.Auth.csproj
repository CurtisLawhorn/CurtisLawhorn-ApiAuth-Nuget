<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild> <!-- Optional -->
    <Authors>Curtis Lawhorn</Authors>
    <Company>Curtis Lawhorn</Company>
    <Description>Middleware library for authenticating and authorizing API requests with Amazon Cognito.</Description>
    <PackageVersion>1.0.1</PackageVersion>
    <PackageId>CurtisLawhorn.Auth</PackageId>
    <RepositoryUrl>https://github.com/CurtisLawhorn/CurtisLawhorn-Auth-Nuget</RepositoryUrl>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.Extensions.NETCore.Setup" Version="4.0.3" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="9.0.9" />
    <PackageReference Include="Microsoft.AspNetCore.Authorization" Version="9.0.9" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.3.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="9.0.9" />
  </ItemGroup>

</Project>
